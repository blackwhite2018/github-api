{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "html",
    "import",
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb/base",
    "eslint-config-prettier"
  ],
  "rules": {
    "no-plusplus": "off",
    "no-console": "off",
    "camelcase": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-unused-vars": "off"
  }
}
